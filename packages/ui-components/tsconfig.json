{
    "compilerOptions": {
        "declaration": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "jsx": "react-native",
        "lib": ["DOM", "ESNext"],
        "moduleResolution": "node",
        "noEmit": true,
        "resolveJsonModule": true,
        "target": "ESNext"
    }
}
